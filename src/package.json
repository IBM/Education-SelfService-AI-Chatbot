{
  "name": "@ibm-watson/assistant-with-discovery",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "async": "^2.6.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^6.2.0",
    "ibm-watson": "^5.5.0",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "replace": "^1.0.0",
    "watson-developer-cloud": "^3.7.0"
  },
  "devDependencies": {
    "codecov": "^3.8.3",
    "csvjson": "^5.1.0",
    "eslint": "^5.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "autofix": "eslint . --fix",
    "build-css": "node-sass-chokidar src/ -o src/",
    "build": "npm run build-css && react-scripts build",
    "codecov": "npm run test && ./node_modules/.bin/codecov",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "start-js": "react-scripts start",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha tests/**/* -- -R spec",
    "train": "cp .env.example .env && node ./train-conversation.js && node ./train-discovery.js",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
